select *
from PortfolioProject.dbo.suspensionraterace2021
where Race_Ethnicity = 'White' AND District = 'Hartford School District'

select *
from PortfolioProject.dbo.suspensionraterace2021
where Race_Ethnicity = 'Black or African American'  AND District = 'Hartford School District'

select *
from PortfolioProject.dbo.suspensionraterace2021
where Race_Ethnicity = 'Hispanic or Latino of any race' AND District = 'Hartford School District'  


